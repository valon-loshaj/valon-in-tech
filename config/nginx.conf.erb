worker_processes 1;

events { worker_connections 1024; }

http {
    server {
        listen 80;
        location / {
            root /usr/share/nginx/html/pages;
            try_files $uri $uri/ /home.html;
        }
        location /about-me {
            try_files $uri /about-me.html;
        }
        location /contact-me {
            try_files $uri /contact-me.html;
        }
    }
}